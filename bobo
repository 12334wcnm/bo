#include <stdio.h>

void main()
{
    int a[10]={100,112,3,4,565,6,77,8,97,140};//定义数组
    float sum=0.0;
    float ave=0.0;
    int i;
    for(i=0;i<10;i++)
    {
        sum+=a[i];
    }
    ave=sum/10.0;
    printf("输出平均值为:%lf\n",ave);
    ave=0.0;
    for(i=0;i<10;i++)
    {
        ave+=a[i]/10.0;
    }
    printf("输出平均值为:%lf\n",ave);
}
